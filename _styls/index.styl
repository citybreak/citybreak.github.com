@import 'nib'
global-reset()
body
  background: url(/assets/images/bg.gif);
  font-size: 14px;

// -----------------MENU-------------------//
.menu-nav
  width: 146px;
  background-color: #422016;
  text-align: center;

.menu-nav .nav-item
  padding: 5px 0;

.menu-nav a
  text-decoration: none;
  color: #f4e7d2;

.menu-nav .nav-link
  font: bold 2em/1 'Oswald', sans-serif;
  text-transform: uppercase;


